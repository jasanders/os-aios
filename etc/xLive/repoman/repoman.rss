<onUserInput>
		userInput = currentUserInput(); 
		if ( userInput == &quot;ENTR&quot; ) 
		{ 
			title = getItemInfo(&quot;title&quot;); 
			writeStringToFile("/tmp/debug", title); 
			if (title == &quot;Dsapac's Adult Section&quot;) 
			{ 
				if(login() == &quot;true&quot;) 
				{ 
					ret = "true"; 
				} 
			} 
		}
