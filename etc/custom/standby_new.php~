#!/usr/local/bin/php
<? 
$sleep_time = intval($_GET['time']);
if ($sleep_time < -1)
{
exec('reboot');
}
if ($sleep_time == 0)
{
exec('echo "FD02FF00" > /sys/devices/platform/VenusIR/fakekey');
exit;
}

$test_com = ('php -f /usr/local/etc/linked/www/cgi-bin/custom/standby_int.php -- '.$_GET['time'].' > /dev/null &');
#file_put_contents("/tmp/command.dat", $test_com);
exec($test_com);
?>
