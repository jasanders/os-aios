	   		pic = "./IMS_Modules/VideoPodcast/image/left.png";
	  	}  	
	  }
	  else if (bLeft == 0)
	  {
	  	if (focusIndex &gt; 7)
	  	{
	  		bLeft = 1;
	   		pic = "./IMS_Modules/VideoPodcast/image/left.png";
